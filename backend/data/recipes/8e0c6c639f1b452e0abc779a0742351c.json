{
  "id": "8e0c6c639f1b452e0abc779a0742351c",
  "title": "Asparagus Parmesan",
  "min_age_month": 12,
  "tags": [
    "正餐",
    "蔬菜",
    "乳制品"
  ],
  "difficulty": 1,
  "time_cost": 0,
  "cover_image": "images/2fdc942b057dc8295ae48aa97f98d06e.webp",
  "nutrition_tip": "Enjoy this cheesy Asparagus Parmesan fresh out of the oven.",
  "ingredients": [
    {
      "name": "asparagus (fresh, washed, trimmed)",
      "amount": "30 spears"
    },
    {
      "name": "button mushrooms (canned)",
      "amount": "4 ounces"
    },
    {
      "name": "onions (green, sliced)",
      "amount": "1 / 4 cup"
    },
    {
      "name": "butter (or margarine)",
      "amount": "2 tablespoons"
    },
    {
      "name": "flour",
      "amount": "2 tablespoons"
    },
    {
      "name": "salt",
      "amount": "1 / 2 teaspoon"
    },
    {
      "name": "milk",
      "amount": "1 / 4 cup"
    },
    {
      "name": "cheese (grated, Parmesan or Cheddar)",
      "amount": "1 / 2 cup"
    }
  ],
  "steps": [
    {
      "step_index": 1,
      "img": "",
      "text": "Preheat oven to 400 degrees F."
    },
    {
      "step_index": 2,
      "img": "",
      "text": "Arrange asparagus in a shallow baking dish."
    },
    {
      "step_index": 3,
      "img": "",
      "text": "Drain mushrooms, reserving liquid. Chop half of mushrooms."
    },
    {
      "step_index": 4,
      "img": "",
      "text": "Cook onions in fat until tender."
    },
    {
      "step_index": 5,
      "img": "",
      "text": "Add flour and salt and blend well."
    },
    {
      "step_index": 6,
      "img": "",
      "text": "Stir in milk and mushroom liquid."
    },
    {
      "step_index": 7,
      "img": "",
      "text": "Cook until thickened, stirring constantly."
    },
    {
      "step_index": 8,
      "img": "",
      "text": "Add chopped mushrooms."
    },
    {
      "step_index": 9,
      "img": "",
      "text": "Pour sauce over asparagus and sprinkle with cheese. Arrange remaining mushrooms on top and sprinkle with cheese. Arrange remaining mushrooms on top."
    },
    {
      "step_index": 10,
      "img": "",
      "text": "Bake 30 to 40 minutes, or until asparagus is tender ."
    }
  ],
  "warnings": [],
