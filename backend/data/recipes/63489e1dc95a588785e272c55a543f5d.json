{
  "id": "63489e1dc95a588785e272c55a543f5d",
  "title": "Homemade Biscuits",
  "min_age_month": 12,
  "tags": [
    "早餐",
    "谷物"
  ],
  "difficulty": 1,
  "time_cost": 10,
  "cover_image": "images/ba37642447a25ad6a814ca956bd62ff5.webp",
  "nutrition_tip": "Make these biscuits from scratch in less than 30 minutes.",
  "ingredients": [
    {
      "name": "flour (all-purpose)",
      "amount": "2 cups"
    },
    {
      "name": "baking powder",
      "amount": "2 teaspoons"
    },
    {
      "name": "baking soda",
      "amount": "1 / 4 teaspoon"
    },
    {
      "name": "salt",
      "amount": "1 / 4 teaspoon"
    },
    {
      "name": "sugar",
      "amount": "2 tablespoons"
    },
    {
      "name": "buttermilk (low-fat (1%))",
      "amount": "2 / 3 cup"
    },
    {
      "name": "vegetable oil",
      "amount": "3 1 / 3 tablespoons"
    }
  ],
  "steps": [
    {
      "step_index": 1,
      "img": "",
      "text": "Preheat oven to 450 °F."
    },
    {
      "step_index": 2,
      "img": "",
      "text": "In a medium bowl, combine flour, baking powder, baking soda, salt, and sugar."
    },
    {
      "step_index": 3,
      "img": "",
      "text": "In a small bowl, stir together buttermilk and oil. Pour over flour mixture and stir until well mixed."
    },
    {
      "step_index": 4,
      "img": "",
      "text": "On a lightly floured surface, knead dough gently for 10–12 strokes. Roll or pat dough to ¾-inch thickness. Cut with a 2-inch round biscuit or cookie cutter, dipping cookie cutter in flour between cuts. Transfer biscuits to an ungreased baking sheet."
    },
    {
      "step_index": 5,
      "img": "",
      "text": "Bake for 12 minutes or until golden brown. Serve warm."
    }
  ],
  "warnings": [],
