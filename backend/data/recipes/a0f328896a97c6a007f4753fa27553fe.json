{
  "id": "a0f328896a97c6a007f4753fa27553fe",
  "title": "Breakfast Burritos",
  "min_age_month": 12,
  "tags": [
    "早餐",
    "乳制品",
    "谷物"
  ],
  "difficulty": 1,
  "time_cost": 10,
  "cover_image": "images/0ef7118c2267f9b92769def9c8649c56.webp",
  "nutrition_tip": "Bring this Breakfast Burrito with you as you start your day.",
  "ingredients": [
    {
      "name": "eggs",
      "amount": "4"
    },
    {
      "name": "milk (1 %)",
      "amount": "1 / 4 cup"
    },
    {
      "name": "salt (to taste)",
      "amount": ""
    },
    {
      "name": "pepper (to taste)",
      "amount": ""
    },
    {
      "name": "chili powder (to taste)",
      "amount": ""
    },
    {
      "name": "oil",
      "amount": "1 teaspoon"
    },
    {
      "name": "tortillas (10-inch flour tortillas)",
      "amount": "4"
    },
    {
      "name": "beans (fat-free refried beans)",
      "amount": "1 cup"
    },
    {
      "name": "cheddar cheese (grated, 4 oz)",
      "amount": "1 / 2 cup"
    },
    {
      "name": "tomato (chopped)",
      "amount": "1"
    }
  ],
  "steps": [
    {
      "step_index": 1,
      "img": "",
      "text": "Mix eggs, milk and seasonings in a bowl."
    },
    {
      "step_index": 2,
      "img": "",
      "text": "Heat oil in skillet over medium-high heat (350 degrees in an electric skillet)."
    },
    {
      "step_index": 3,
      "img": "",
      "text": "Stir in the eggs and cook until firm."
    },
    {
      "step_index": 4,
      "img": "",
      "text": "Warm the tortillas on a griddle, or wrap in foil and heat in the oven."
    },
    {
      "step_index": 5,
      "img": "",
      "text": "Warm the refried beans in a separate pan."
    },
    {
      "step_index": 6,
      "img": "",
      "text": "On each tortilla, layer 1/4 of refried beans, eggs, cheese, and tomato."
    },
    {
      "step_index": 7,
      "img": "",
      "text": "Roll the burrito, cut in half, and enjoy!"
    },
    {
      "step_index": 8,
      "img": "",
      "text": "Refrigerate leftovers within 2 hours."
    }
  ],
