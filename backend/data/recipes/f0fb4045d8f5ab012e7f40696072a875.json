  "title": "Pineapple Frozen Fruit Pops",
  "min_age_month": 8,
  "tags": [
    "点心",
    "水果",
    "乳制品"
  ],
  "difficulty": 1,
  "time_cost": 0,
  "cover_image": "images/b360485872d95e7b86ca3b6625399699.webp",
  "nutrition_tip": "These frozen fruit pops contain fruit, juice, and low-fat yogurt to create a ref…",
  "ingredients": [
    {
      "name": "pineapple (crushed)",
      "amount": "1 cup"
    },
    {
      "name": "yogurt (low-fat, fruit flavored)",
      "amount": "1 cup"
    },
    {
      "name": "orange juice (frozen, concentrate (thawed))",
      "amount": "6 ounces"
    }
  ],
  "steps": [
    {
      "step_index": 1,
      "img": "",
      "text": "Mix the ingredients in a medium-size bowl. Divide into 4 paper cups."
    },
    {
      "step_index": 2,
      "img": "",
      "text": "Freeze until slushy - about 60 minutes. Insert a wooden stick half way through the center of each fruit pop."
    },
    {
      "step_index": 3,
      "img": "",
      "text": "Freeze until hard or at least 4 hours. Peel away the paper cup before you eat the fruit pop."
    }
  ],
