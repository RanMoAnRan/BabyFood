  ],
  "steps": [
    {
      "step_index": 1,
      "img": "",
      "text": "Heat oven to 350 degrees."
    },
    {
      "step_index": 2,
      "img": "",
      "text": "In a large bowl, mix sugars, oil, applesauce, eggs, and vanilla thoroughly."
    },
    {
      "step_index": 3,
      "img": "",
      "text": "In a separate bowl, stir dry ingredients together."
    },
    {
      "step_index": 4,
      "img": "",
      "text": "Blend dry ingredients into wet mixture. Stir in raisins and carrots."
    },
    {
      "step_index": 5,
      "img": "",
      "text": "Drop by teaspoonfuls on greased baking sheet."
    },
    {
      "step_index": 6,
      "img": "",
      "text": "Bake 12 to 15 minutes until golden brown."
    },
    {
      "step_index": 7,
      "img": "",
      "text": "Store in airtight container."
    }
  ],
