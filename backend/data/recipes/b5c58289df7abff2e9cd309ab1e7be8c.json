      "name": "地面火鸡",
      "amount": "1 磅"
    },
    {
      "name": "帕尔马干酪",
      "amount": "1 / 4 杯"
    },
    {
      "name": "切碎的蔬菜（西红柿、青豆、胡萝卜、辣椒、西葫芦等）",
      "amount": "2 杯"
    },
    {
      "name": "全麦玉米饼",
      "amount": "8"
    }
