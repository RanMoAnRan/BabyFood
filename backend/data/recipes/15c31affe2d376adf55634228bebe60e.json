  "title": "Black Bean Quesadilla",
  "min_age_month": 12,
  "tags": [
    "正餐",
    "点心",
    "蔬菜",
    "蛋白质",
    "谷物"
  ],
  "difficulty": 1,
  "time_cost": 0,
  "cover_image": "images/1a70afe2d98998fb4bd530b362e41c6e.webp",
  "nutrition_tip": "Enjoy a quesadilla any night of the week! This quesadilla recipe can be served a…",
  "ingredients": [
    {
      "name": "salsa (chuncky or pico de gallo)",
      "amount": "3 / 4 cup"
    },
    {
      "name": "black beans (15.5 oz, drained and rinsed)",
      "amount": "1 can"
    },
    {
      "name": "cheese (Colby &amp; Monterey Jack, reduced-fat, shredded)",
      "amount": "2 cups"
    },
    {
      "name": "cilantro (fresh, finely chopped)",
      "amount": "2 tablespoons"
    },
    {
      "name": "tortillas (flour)",
      "amount": "4 large"
    },
    {
      "name": "olive oil",
      "amount": "1 / 2 teaspoon"
    }
  ],
  "steps": [
    {
      "step_index": 1,
      "img": "",
      "text": "Using a small-hole strainer, drain liquid from salsa; discard liquid."
    },
    {
      "step_index": 2,
      "img": "",
      "text": "Transfer leftover tomato mixture to a medium bowl."
    },
    {
      "step_index": 3,
      "img": "",
      "text": "Mix in black beans, cheese, and cilantro until combined."
    },
    {
      "step_index": 4,
      "img": "",
      "text": "Divide black bean mixture evenly over half of each tortilla (about 1/2 cup each)."
    },
    {
      "step_index": 5,
      "img": "",
      "text": "Fold tortillas in half."
    },
    {
      "step_index": 6,
      "img": "",
      "text": "Heat large griddle or skillet over medium-high heat."
    },
    {
      "step_index": 7,
      "img": "",
      "text": "Brush with oil."
    },
    {
      "step_index": 8,
      "img": "",
      "text": "Place filled tortillas on a griddle."
    },
    {
      "step_index": 9,
      "img": "",
      "text": "Cook, carefully flipping once, until tortillas are gold brown and crisp and cheese filling melts, about 5 minutes."
    },
    {
      "step_index": 10,
      "img": "",
      "text": "Cut quesadillas into wedges."
    }
  ],
