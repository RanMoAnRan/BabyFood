  "title": "Chicken Enchiladas",
  "min_age_month": 12,
  "tags": [
    "正餐",
    "蔬菜",
    "蛋白质",
    "谷物"
  ],
  "difficulty": 1,
  "time_cost": 15,
  "cover_image": "images/0ba168a2bd677dfd0d89eb385972d16d.webp",
  "nutrition_tip": "Packed with tender chicken, cabbage, onions and cheese, these flavorful enchilad…",
  "ingredients": [
    {
      "name": "chicken (chopped, cooked)",
      "amount": "9 ounces"
    },
    {
      "name": "onion (chopped)",
      "amount": "1 small"
    },
    {
      "name": "mozzarella (shredded, part-skim)",
      "amount": "1 / 2 cup"
    },
    {
      "name": "cabbage (shredded)",
      "amount": "2 cups"
    },
    {
      "name": "tortillas (8-inch)",
      "amount": "5"
    },
    {
      "name": "diced tomatoes with green chilies (lightly drained)",
      "amount": "1 can"
    },
    {
      "name": "parmesan cheese",
      "amount": "1 / 4 cup"
    },
    {
      "name": "Nonstick spray",
      "amount": ""
    }
  ],
  "steps": [
    {
      "step_index": 1,
      "img": "",
      "text": "Preheat oven to 350°F."
    },
    {
      "step_index": 2,
      "img": "",
      "text": "Spray 9 by 12” baking dish with nonstick spray."
    },
    {
      "step_index": 3,
      "img": "",
      "text": "Combine chicken, onion, cabbage and mozzarella in a large bowl."
    },
    {
      "step_index": 4,
      "img": "",
      "text": "Place about ½ cup chicken mixture on tortilla, spreading mixture to form a line across largest width."
    },
    {
      "step_index": 5,
      "img": "",
      "text": "Roll ingredients in tortilla and place in a baking dish."
    },
    {
      "step_index": 6,
      "img": "",
      "text": "Top tortillas with tomato and sprinkle with Parmesan cheese."
    },
    {
      "step_index": 7,
      "img": "",
      "text": "Cover with aluminum foil."
    },
    {
      "step_index": 8,
      "img": "",
      "text": "Bake for 30 minutes."
    }
  ],
