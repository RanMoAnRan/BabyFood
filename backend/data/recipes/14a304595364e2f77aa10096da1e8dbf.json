      "amount": "1 tablespoon"
    },
    {
      "name": "lemon juice",
      "amount": "2 tablespoons"
    },
    {
      "name": "cabbage (red and green, shredded)",
      "amount": "2 cups"
    },
    {
      "name": "tomatoes (diced)",
      "amount": "2 cups"
    },
    {
      "name": "tortillas (6-inch, warmed)",
      "amount": "12 medium"
    },
    {
      "name": "lime (wedges, for serving )",
      "amount": "1 small"
    }
