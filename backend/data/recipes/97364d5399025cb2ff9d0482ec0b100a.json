{
  "id": "97364d5399025cb2ff9d0482ec0b100a",
  "title": "White Bean and Egg Shakshuka",
  "min_age_month": 12,
  "tags": [
    "早餐",
    "蔬菜",
    "蛋白质"
  ],
  "difficulty": 1,
  "time_cost": 5,
  "cover_image": "images/deb21c358f64e53104e40ee8f376273e.webp",
  "nutrition_tip": "Never skip breakfast again with this quick microwavable recipe.&nbsp;",
  "ingredients": [
    {
      "name": "tomato sauce (no-salt added)",
      "amount": "1 / 4 cup"
    },
    {
      "name": "beans (white (e.g. Cannellini, Great Northern), drained and rinsed)",
      "amount": "1 / 4 cup"
    },
    {
      "name": "thyme (dried)",
      "amount": "1 / 4 teaspoon"
    },
    {
      "name": "paprika (sweet or smoked)",
      "amount": "1 / 4 teaspoon"
    },
    {
      "name": "onion powder",
      "amount": "1 / 4 teaspoon"
    },
    {
      "name": "garlic powder",
      "amount": "1 pinch"
    },
    {
      "name": "salt",
      "amount": "1 pinch"
    },
    {
      "name": "Ground black pepper",
      "amount": "1 pinch"
    },
    {
      "name": "eggs",
      "amount": "2"
    }
  ],
  "steps": [
    {
      "step_index": 1,
      "img": "",
      "text": "Coat the inside of a microwave-safe bowl with cooking spray."
    },
    {
      "step_index": 2,
      "img": "",
      "text": "Add the tomato sauce, beans, thyme, paprika, onion powder, garlic powder, salt, and black pepper. Stir to combine."
    },
    {
      "step_index": 3,
      "img": "",
      "text": "Make a well in the center of the sauce, and crack the eggs into it."
    },
    {
      "step_index": 4,
      "img": "",
      "text": "Microwave on high power (default setting) for 1 minute."
    },
    {
      "step_index": 5,
      "img": "",
      "text": "Continue cooking in 20-second bursts at 80% power until the egg yolks and whites are set. Depending on your microwave, this will take about 2-4 minutes in total."
    },
    {
      "step_index": 6,
      "img": "",
      "text": "Serve warm."
    }
  ],
