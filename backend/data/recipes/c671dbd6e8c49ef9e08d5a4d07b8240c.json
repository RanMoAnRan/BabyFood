{
  "id": "c671dbd6e8c49ef9e08d5a4d07b8240c",
  "title": "烤鸡",
  "min_age_month": 12,
  "tags": [
    "晚餐",
    "蛋白质"
  ],
  "difficulty": 1,
  "time_cost": 0,
  "cover_image": "images/161791942adbe3916b2a6f8778c3e030.webp",
  "nutrition_tip": "将这只烤鸡作为晚餐的中心菜肴，或将其添加到沙拉、沙子中......",
  "ingredients": [
    {
      "name": "意大利调味料",
      "amount": "1 汤匙"
    },
    {
      "name": "大蒜粉",
      "amount": "1 茶匙"
    },
    {
      "name": "盐和胡椒（适量）",
      "amount": ""
    },
    {
      "name": "整只鸡",
      "amount": "1"
    },
    {
      "name": "橄榄油（或菜籽油）",
      "amount": "1 汤匙"
    }
  ],
  "steps": [
    {
      "step_index": 1,
      "img": "",
      "text": "将烤箱预热至 375 华氏度。"
    },
    {
      "step_index": 2,
      "img": "",
      "text": "将意大利调味料、大蒜粉、盐和胡椒混合。"
    },
    {
      "step_index": 3,
      "img": "",
      "text": "用油擦鸡肉，然后用香料混合物擦。"
    },
    {
      "step_index": 4,
      "img": "",
      "text": "将鸡胸肉面朝下放入荷兰烤箱或烤盘中。"
    },
    {
      "step_index": 5,
      "img": "",
      "text": "不加盖地烘烤，直到内部温度至少达到 165 华氏度。"
    },
    {
      "step_index": 6,
      "img": "",
      "text": "让鸡肉休息十分钟后再切。"
    }
  ],
