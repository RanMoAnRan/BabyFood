  "nutrition_tip": "Enjoy a healthier version of an old dinner favorite.",
  "ingredients": [
    {
      "name": "ground turkey (lean)",
      "amount": "1 pound"
    },
    {
      "name": "rolled oats (dry)",
      "amount": "1 / 2 cup"
    },
    {
      "name": "egg",
      "amount": "1 large"
    },
    {
      "name": "onion (dehydrated [or 1 small onion, minced])",
      "amount": "1 tablespoon"
    },
    {
      "name": "ketchup",
      "amount": "1 / 4 cup"
    },
    {
      "name": "celery (chopped)",
      "amount": "2 stalks"
    },
    {
      "name": "garlic (minced)",
      "amount": "2 cloves"
    },
    {
      "name": "bell pepper (green, seeded and diced)",
      "amount": "1 / 2"
    }
  ],
  "steps": [
    {
      "step_index": 1,
      "img": "",
      "text": "Preheat oven to 350°F."
    },
    {
      "step_index": 2,
      "img": "",
      "text": "Combine all ingredients and mix well."
    },
    {
      "step_index": 3,
      "img": "",
      "text": "Bake in loaf pan for 25 minutes (to a minimum internal temperature of 165°F)."
    },
    {
      "step_index": 4,
      "img": "",
      "text": "Cut into five slices and serve."
    }
  ],
