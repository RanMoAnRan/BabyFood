  "id": "1e2649acdd25ebee0fd6d903e3d5b0f0",
  "title": "One Pan Spaghetti",
  "min_age_month": 12,
  "tags": [
    "正餐",
    "蔬菜",
    "蛋白质",
    "乳制品",
    "谷物"
  ],
  "difficulty": 1,
  "time_cost": 10,
  "cover_image": "images/527258600c6d646e807b999139384079.webp",
  "nutrition_tip": "This cheesy pasta recipe is easy to make, and kids love it!",
  "ingredients": [
    {
      "name": "ground beef (lean, 15% fat)",
      "amount": "1 / 2 pound"
    },
    {
      "name": "onion (chopped)",
      "amount": "1 medium"
    },
    {
      "name": "water",
      "amount": "3 cups"
    },
    {
      "name": "tomato sauce (15 ounces)",
      "amount": "1 can"
    },
    {
      "name": "oregano (dried)",
      "amount": "2 teaspoons"
    },
    {
      "name": "sugar",
      "amount": "1 teaspoon"
    },
    {
      "name": "garlic powder (or 2 garlic cloves)",
      "amount": "1 teaspoon"
    },
    {
      "name": "rosemary",
      "amount": "1 teaspoon"
    },
    {
      "name": "black pepper",
      "amount": "1 / 4 teaspoon"
    },
    {
      "name": "spaghetti (12 ounces, broken)",
      "amount": "2 cups"
    },
    {
      "name": "parmesan cheese (4 oz, shredded)",
      "amount": "1 cup"
    }
  ],
  "steps": [
    {
      "step_index": 1,
      "img": "",
      "text": "Brown meat and onions in a large skillet over medium-high heat (300 degrees in an electric skillet). Drain fat."
    },
    {
      "step_index": 2,
      "img": "",
      "text": "Stir in water, tomato sauce, and spices. Bring to a boil."
    },
    {
      "step_index": 3,
      "img": "",
      "text": "Add spaghetti, cover pan, and simmer 10-15 minutes, stirring often to prevent sticking."
    },
    {
      "step_index": 4,
      "img": "",
      "text": "When spaghetti is tender, top with grated cheese."
    },
    {
      "step_index": 5,
      "img": "",
      "text": "Refrigerate leftovers within 2 hours."
    }
  ],
