  "title": "Pumpkin Pancakes",
  "min_age_month": 6,
  "tags": [
    "早餐",
    "蔬菜",
    "谷物"
  ],
  "difficulty": 1,
  "time_cost": 0,
  "cover_image": "images/3899c823645cc40a1ba47a4476ef4aae.webp",
  "nutrition_tip": "With hints of pumpkin pie spice and brown sugar, these mouthwatering pancakes ca…",
  "ingredients": [
    {
      "name": "flour",
      "amount": "1 cup"
    },
    {
      "name": "whole wheat flour",
      "amount": "1 cup"
    },
    {
      "name": "baking powder",
      "amount": "2 teaspoons"
    },
    {
      "name": "salt",
      "amount": "1 / 2 teaspoon"
    },
    {
      "name": "brown sugar",
      "amount": "2 tablespoons"
    },
    {
      "name": "pumpkin pie spice",
      "amount": "1 teaspoon"
    },
    {
      "name": "pumpkin puree (canned)",
      "amount": "3 / 4 cup"
    },
    {
      "name": "milk (nonfat)",
      "amount": "1 3 / 4 cups"
    },
    {
      "name": "vegetable oil",
      "amount": "3 tablespoons"
    },
    {
      "name": "eggs (lightly beaten)",
      "amount": "3"
    }
  ],
  "steps": [
    {
      "step_index": 1,
      "img": "",
      "text": "Wash hands with soap and water."
    },
    {
      "step_index": 2,
      "img": "",
      "text": "In a large bowl combine flours, baking powder, salt, brown sugar, and pumpkin pie spice using a wire whisk."
    },
    {
      "step_index": 3,
      "img": "",
      "text": "In another bowl combine pumpkin, milk, oil and eggs. Mix until smooth."
    },
    {
      "step_index": 4,
      "img": "",
      "text": "Stir pumpkin mixture into the dry ingredients, mixing until moistened."
    },
    {
      "step_index": 5,
      "img": "",
      "text": "Spoon the batter onto a slightly greased, preheated skillet."
    },
    {
      "step_index": 6,
      "img": "",
      "text": "Cook slowly until bubbles appear on top and bottom becomes golden brown."
    },
    {
      "step_index": 7,
      "img": "",
      "text": "Turn pancakes and cook until other side is golden brown."
    },
    {
      "step_index": 8,
      "img": "",
      "text": "Serve warm. Top with powdered sugar or pancake syrup."
    }
  ],
